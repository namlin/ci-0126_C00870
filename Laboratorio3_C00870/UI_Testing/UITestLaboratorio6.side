{
  "id": "create-country-test",
  "version": "2.0",
  "name": "Create Country Test",
  "url": "http://localhost:8080",
  "tests": [
    {
      "id": "form-country-test",
      "name": "Formulario Crear País",
      "commands": [
        {
          "command": "open",
          "target": "/country",
          "value": ""
        },

        {
          "command": "assertText",
          "target": "css=h3.text-center",
          "value": "Formulario de creación de países"
        },

        {
          "command": "assertElementPresent",
          "target": "css=input#name",
          "value": ""
        },

        {
          "command": "assertElementPresent",
          "target": "css=select#continente",
          "value": ""
        },

        {
          "command": "assertElementPresent",
          "target": "css=input#idioma",
          "value": ""
        },

        {
          "command": "type",
          "target": "css=input#name",
          "value": "Colombia"
        },
        {
          "command": "assertValue",
          "target": "css=input#name",
          "value": "Colombia"
        },

        {
          "command": "select",
          "target": "css=select#continente",
          "value": "label=América"
        },
        {
          "command": "assertValue",
          "target": "css=select#continente",
          "value": "América"
        },

        {
          "command": "type",
          "target": "css=input#idioma",
          "value": "Español"
        },
        {
          "command": "assertValue",
          "target": "css=input#idioma",
          "value": "Español"
        },

        {
          "command": "click",
          "target": "css=button.btn-success",
          "value": ""
        },

        {
          "command": "pause",
          "target": "2000",
          "value": ""
        }
      ]
    }
  ]
}
